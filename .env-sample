####################################
# web configuration
#  1. copy me to .env and configure
#  2. be sure to run with foreman
####################################

# environment information
RACK_ENV=development

# where shall we look for the stream server? (no trailing slash)
STREAM_SERVER=https://stream.emojitracker.com

# where shall we look for the API server? (no trailing slash)
API_SERVER=https://api.emojitracker.com/v1

# how many unicorn workers should we run?
WEB_CONCURRENCY=4

#what port to run on
PORT=8000

#logs
VERBOSE=true
